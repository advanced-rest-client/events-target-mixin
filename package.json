{
  "description": "Mixin that support event targets retargeting so the element listens on a set node instead of default one",
  "keywords": [
    "web-components",
    "polymer"
  ],
  "repository": {
    "type": "git",
    "url": "git://github.com/advanced-rest-client/events-target-mixin.git"
  },
  "name": "@advanced-rest-client/events-target-mixin",
  "version": "3.0.0",
  "license": "Apache-2.0",
  "devDependencies": {
    "@advanced-rest-client/testing-karma-sl": "^1.0.2",
    "@open-wc/testing": "^0.11.1",
    "@open-wc/testing-karma": "^2.0.16",
    "@polymer/gen-typescript-declarations": "^1.6.1",
    "lit-element": "^2.2.0"
  },
  "main": "events-target-mixin.js",
  "author": "The Advanced REST client authors <arc@mulesoft.com>",
  "scripts": {
    "test": "karma start --coverage",
    "test:watch": "karma start --auto-watch=true --single-run=false",
